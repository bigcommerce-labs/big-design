{
  "name": "@bigcommerce/big-design-icons",
  "version": "0.17.1",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "BigCommerce Inc",
  "repository": {
    "type": "git",
    "url": "https://github.com/bigcommerce/big-design.git",
    "directory": "packages/big-design-icons"
  },
  "scripts": {
    "build": "yarn run build:cjs && yarn run build:es && yarn run build:dt",
    "build:cjs": "NODE_ENV=production swc --config module.type=commonjs --extensions \".ts,.tsx\" ./src --out-dir ./dist/cjs",
    "build:es": "NODE_ENV=production swc --extensions \".ts,.tsx\" ./src --out-dir ./dist/es",
    "build:dt": "tsc --emitDeclarationOnly",
    "build:icons": "node scripts/build.js",
    "build:flags": "node scripts/build-flags.js",
    "download": "node scripts/downloader.js",
    "prepublishOnly": "rimraf dist && yarn run typeCheck && yarn run build",
    "typeCheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "flags",
    "svgs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bigcommerce/big-design-theme": "^0.13.1",
    "@swc/helpers": "^0.2.13",
    "react-uid": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@bigcommerce/configs": "^0.15.1",
    "@jorgemoya/flag-icon-css": "3.5.0",
    "@svgr/core": "^5.0.1",
    "@svgr/plugin-jsx": "^5.0.1",
    "@svgr/plugin-prettier": "^5.0.1",
    "@svgr/plugin-svgo": "^5.0.1",
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.95",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.11",
    "browserslist": "^4.17.4",
    "camelcase": "^6.2.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "glob-promise": "^3.4.0",
    "inquirer": "^7.0.4",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "node-fetch": "^2.6.0",
    "prettier": "^2.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.0",
    "tiny-async-pool": "^1.2.0",
    "typescript": "^4.4.3",
    "typescript-styled-plugin": "^0.18.1"
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Safari major versions",
    "last 2 Edge major versions",
    "last 2 Android major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ]
}
